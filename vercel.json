{
  "outputDirectory": "client/build",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://kudimall.onrender.com/api/:path*"
    },
    {
      "source": "/uploads/:path*",
      "destination": "https://kudimall.onrender.com/uploads/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
